{
    "name": "exercise/htmlpurifier-bundle",
    "type": "symfony-bundle",
    "description": "HTMLPurifier integration for your Symfony project",
    "keywords": ["htmlpurifier", "html", "purifier", "symfony2", "symfony3", "symfony4"],
    "homepage": "https://github.com/HeahProd/HTMLPurifierBundle",
    "license": "MIT",
    "authors": [
        {"name": "contributors", "homepage": "https://github.com/Exercise/HTMLPurifierBundle/contributors" }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": "~2.8.32|3.4.1|~4.0.1",
        "ezyang/htmlpurifier": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.4",
        "symfony/form": "~2.8.32|3.4.1|^4.0.1",
        "twig/twig": "~1.6.5|^2.4.4"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": { "Exercise\\HTMLPurifierBundle\\": "" }
    }
}
